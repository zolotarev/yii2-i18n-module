{
    "name": "zolotarev/yii2-i18n-module",
    "description": "EXTENDED Yii2 i18n (internalization) module makes the translation of your application so simple",
    "version": "0.1.9",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "i18n",
        "translation",
        "translations"
    ],
    "homepage": "https://github.com/zolotarev/yii2-i18n-module",
    "time": "2015-03-07",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Zolotarev",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zolotarev/yii2-i18n-module/issues",
        "source": "https://github.com/zolotarev/yii2-i18n-module"
    },
    "require": {
        "yiisoft/yii2": "~2",
        "zelenin/yii2-semantic-ui": "*"
    },
    "autoload": {
        "psr-4": {
            "Zelenin\\yii\\modules\\I18n\\": ""
        }
    },
    "extra": {
        "bootstrap": "Zolotarev\\yii\\modules\\I18n\\Bootstrap"
    }
}
